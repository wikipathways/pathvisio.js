{
  "name": "pvjs",
  "version": "2.4.84",
  "description": "Diagram viewer and editor for biological pathways.",
  "main": "./index.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "directories": {
    "test": "test",
    "packages": "jspm_packages"
  },
  "scripts": {
    "build": "browserify ./lib/custom-element.js -o ./test/lib/pvjs/dev/pvjs.core.js -d -t brfs --ignore=jquery && echo 'Build complete...'",
    "build:min": "browserify ./lib/custom-element.js -o ./test/lib/pvjs/dev/pvjs.core.min.js -d -t brfs -t deglobalify --ignore=jquery",
    "build:watch": "watch --filter=watch-filter.js --ignoreDotFile --ignoreUnreadable --wait=2 'npm run build' .",
    "start-broken": "npm-run-all build --parallel start:server build:watch",
    "start": "echo 'Manually start with npm run start:server and, in another terminal, npm run build:watch'",
    "start:server": "echo 'Go to http://localhost:8080/test/' && http-server",
    "test": "npm run test:unit",
    "test:integration": "gulp testLocalhost",
    "test:unit": "mocha ./test/spec/api.js --timeout=10000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikipathways/pvjs.git"
  },
  "keywords": [
    "pathway",
    "network",
    "biology",
    "node",
    "edge",
    "diagram"
  ],
  "author": "Pvjs team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wikipathways/pvjs/issues"
  },
  "homepage": "http://wikipathways.github.io/pvjs",
  "gitHead": "044be6618ee72b932fcb90f4369eb61706f3cfb9",
  "readmeFilename": "README.md",
  "browser": {
    "cheerio": false,
    "commander": false,
    "d3": false,
    "jquery": false,
    "rx": "rx-extra"
  },
  "browserify": {
    "transform": [
      "brfs",
      "deglobalify"
    ],
    "ignore": [
      "jquery"
